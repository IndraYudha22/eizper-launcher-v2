        <ImageBrush x:Key="ig_icon" ImageSource="ImagesHover/hover_ig.png" Stretch="Uniform"/>
        <ImageBrush x:Key="web_icon" ImageSource="ImagesHover/hover_web.png" Stretch="Uniform"/>
        <ImageBrush x:Key="med_icon" ImageSource="ImagesHover/hover_med.png" Stretch="Uniform"/>
        <ImageBrush x:Key="fb_icon" ImageSource="ImagesHover/hover_fb.png" Stretch="Uniform"/>

